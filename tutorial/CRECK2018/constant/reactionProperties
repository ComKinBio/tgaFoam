// cellulose
//CELL=>CELLA
reaction0
{
  reactants CELL;

  solidProducts
  (
    (CELLA 1)
  );

  gasProducts
  (
    (Null 0)
  );

  Ea  47000.00;
  A 1.50000E+014;
  b 0;

};

//CELLA=> 0.05*CH2OHCH2CHO + 0.4*CH2OHCHO
// " 0.03*CHOCHO + 0.17*CH3CHO + 0.25*C6H6O3 + 0.35*C2H5CHO
// " 0.2*CH3OH + 0.15*CH2O + 0.49*CO + 0.43*CO2 + 0.13*H2
// " 0.93*H2O + 0.02*HCOOH + 0.05*CH4
//+ 0.66*CHAR + 0.05*G_CO + 0.05*G_COH2_L + 0.1*G_H2"
reaction1
{
  reactants CELLA;

  solidProducts
  (
      (C 0.66)
      (GCO 0.05)
      (GCOH2L 0.05)
      (GH2 0.1)
  );

  gasProducts
  (
      (CH2OHCH2CHO 0.05)
      (CH2OHCHO 0.4)
      (CHOCHO 0.03)
      (CH3CHO 0.17)
      (C6H6O3 0.25)
      (C2H5CHO 0.35)
      (CH3OH 0.2)
      (CH2O 0.15)
      (CO 0.49)
      (CO2 0.43)
      (H2 0.13)
      (H2O 0.93)
      (HCOOH 0.02)
      (CH4 0.05)
  );

    Ea  19100.00;
    A 2.5000E+006;
    b 0;
};

