FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "90";
    object      dQ;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 2 -3 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    WALL
    {
        type            calculated;
        value           uniform 0;
    }
    INLET
    {
        type            calculated;
        value           uniform 0;
    }
    OUTLET
    {
        type            calculated;
        value           uniform 0;
    }
}


