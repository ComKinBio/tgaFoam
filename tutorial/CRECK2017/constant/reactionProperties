// cellulose
//CELL=>CELLA
reaction0
{
  reactants CELL;

  solidProducts
  (
    (CELLA 1)
  );
  
  gasProducts
  (
    (CO2 0)
  );

  Ea  47000.00;
  A 1.50000E+014;
  b 0;

};

//CELLA=>  0.45 C2H4O2 + 0.2 C2H2O2 + 0.1 CH3CHO + 0.25 HMFU + 0.3 C2H5CHO + 0.15 CH3OH + 0.4 CH2O + 0.31 CO
//       + 0.41 CO2 + 0.05 H2 + 0.83 H2O + 0.02 HCOOH + 0.2 G{CH4} + 0.05 G{H2} + 0.61 CHAR
reaction1
{
  reactants CELLA;

  solidProducts
  (
      (C 0.61)
      (GCH4 0.2)
      (GH2 0.05)
  );

  gasProducts
  (
      (C2H4O2 0.45)
      (C2H2O2 0.2)
      (CH3CHO 0.1)
      (C6H6O3 0.25)
      (C2H5CHO 0.3)
      (CH3OH 0.15)
      (CH2O 0.4)
      (CO 0.31)
      (CO2 0.41)
      (H2 0.05)
      (H2O 0.83)
      (HCOOH 0.02)
  );

    Ea  19100.00;
    A 2.0000E+006;
    b 0;
};

