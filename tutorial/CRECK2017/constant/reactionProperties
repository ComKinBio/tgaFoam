//HCE1=>0.025 H2O + 0.5 CO2 + 0.025 HCOOH + 0.5 CO + 0.8 CH2O + 0.125 C2H5OH + 0.1 CH3OH + 0.25 C2H4 
//    + 0.125 G{H2} + 0.275 G{CO2} + 0.4 G{COH2} + 0.45 G{CH3OH} + 0.325 G{CH4} + 0.875 CHAR
reaction5
{
  reactants HCE1;

  solidProducts
  (
      (GH2 0.125)
      (GCO2 0.275)
      (GCOH2 0.4)
      (GCH3OH 0.45)
      (GCH4 0.325)
      (C 0.875)
  );

  gasProducts
  (
      (H2O 0.025)
      (CO2 0.5)
      (HCOOH 0.025)
      (CO 0.5)
      (CH2O 0.8)
      (C2H5OH 0.125)
      (CH3OH 0.1)
      (C2H4 0.25)
  );

    Ea  30000.00;
    A 1.2000E+009;
    b 0;
  
};

//HCE1=>0.25 H2O + 0.8 CO2 + 0.05 HCOOH + 0.1 CO + 0.3 CH2O +
